{
  "name": "imard_module_repo",
  "version": "1.0.0",
  "description": "imard module repository for cloning, building and saving to the CDN all modules",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "pretest": "node self_modules/recourse/toggleTest.js",
    "posttest": "node self_modules/recourse/toggleProduction.js",
